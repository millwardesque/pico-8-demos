pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

ring = {
 x = 32,
 y = 44,
 width = 64,
 height = 40,
 apron_height = 6,
 post_width = 2,
 post_height = 10,
 rope_width = 1,
 colours = {
  mat = 6,
  apron = 1,
  post = 12,
  top_rope = 8,
  middle_rope = 7,
  bottom_rope = 8,
 },
}

function ring:on_draw()
 -- draw the back posts
 -- back-left
 draw_rect(self.x - flr(self.post_width / 2), self.y - self.post_height, self.post_width, self.post_height, self.colours.post)

 -- back-right
 draw_rect(self.x + self.width - flr(self.post_width / 2), self.y - self.post_height, self.post_width, self.post_height, self.colours.post)

-- draw the mat
 draw_rect(self.x, self.y, self.width, self.height, self.colours.mat)

 -- draw the ropes
 -- back
 local rope_top = self.y - self.post_height + 1
 local distance_between_ropes = 3
 draw_rect(self.x, rope_top - flr(self.rope_width / 2), self.width, self.rope_width, self.colours.top_rope)
 draw_rect(self.x, distance_between_ropes + rope_top - flr(self.rope_width / 2), self.width, self.rope_width, self.colours.middle_rope)
 draw_rect(self.x, 2 * distance_between_ropes + rope_top - flr(self.rope_width / 2), self.width, self.rope_width, self.colours.bottom_rope)

 -- sides
 draw_rect(self.x, rope_top - flr(self.rope_width / 2), self.rope_width, self.height, self.colours.top_rope)
 draw_rect(self.x + self.width - 1, rope_top - flr(self.rope_width / 2), self.rope_width, self.height, self.colours.top_rope)

 -- front
 rope_top = self.y + self.height - self.post_height + 1
 draw_rect(self.x, rope_top - flr(self.rope_width / 2), self.width, self.rope_width, self.colours.top_rope)
 draw_rect(self.x, distance_between_ropes + rope_top - flr(self.rope_width / 2), self.width, self.rope_width, self.colours.middle_rope)
 draw_rect(self.x, 2 * distance_between_ropes + rope_top - flr(self.rope_width / 2), self.width, self.rope_width, self.colours.bottom_rope)

 -- draw the apron
 draw_rect(self.x, self.y + self.height, self.width, self.apron_height, self.colours.apron)

 -- draw the front posts
 -- front-left
 draw_rect(self.x - flr(self.post_width / 2), self.y + self.height - self.post_height, self.post_width, self.post_height, self.colours.post)
 
 -- front-right
 draw_rect(self.x + self.width - flr(self.post_width / 2), self.y + self.height - self.post_height, self.post_width, self.post_height, self.colours.post)
end

base_wrestler_colours = {
 face = 1,
 torso = 2,
 arms = 3,
 legs = 4,
 eyes = 5,
 hair = 6,
 hair_long = 8,
 face_paint = 9,
 arm_band = 10,
 boots = 11,
 mouth = 12,
}

common_wrestler_colours = {
 hulk = {
  trans = 0,
  face = 9,
  torso = 9,
  arms = 15,
  legs = 8,
  eyes = 7,
  hair = 10,
  hair_long = 10,
  face_paint = 9,
  arm_band = 15,
  boots = 10,
  mouth = 10,
 },
 sting = {
  trans = 0,
  face = 9,
  torso = 9,
  arms = 15,
  legs = 11,
  eyes = 7,
  hair = 7,
  hair_long = 0,
  face_paint = 11,
  arm_band = 15,
  boots = 7,
  mouth = 9,
 },
 vader = {
  trans = 14,
  face = 15,
  torso = 8,
  arms = 15,
  legs = 0,
  eyes = 7,
  hair = 0,
  hair_long = 14,
  face_paint = 8,
  arm_band = 15,
